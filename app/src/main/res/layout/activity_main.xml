<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/messenger_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/messenger_button"/>

    <Button
        android:id="@+id/aidl_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/aidl_button"/>

    <Button
        android:id="@+id/provider_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/provider_button"/>

    <Button
        android:id="@+id/binder_pool_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/binder_pool_button"/>

</LinearLayout>